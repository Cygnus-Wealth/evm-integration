{
  "name": "@cygnus-wealth/evm-integration",
  "version": "1.1.0",
  "description": "Framework-agnostic TypeScript library for read-only EVM blockchain data access",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run src/registry/ChainRegistry.e2e.test.ts",
    "verify:balance": "tsx src/e2e/verifyBalance.ts",
    "verify:websocket": "tsx src/e2e/websocketVerification.ts",
    "demo": "cd demo-ui && npm install && npm run dev",
    "dev:ui": "cd test-ui && npm install && npm run dev",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "evm",
    "web3",
    "cygnus-wealth"
  ],
  "author": "Jacob Boswell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cygnus-Wealth/evm-integration.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@cygnus-wealth/data-models": "^1.1.0",
    "viem": "^2.32.1"
  }
}
